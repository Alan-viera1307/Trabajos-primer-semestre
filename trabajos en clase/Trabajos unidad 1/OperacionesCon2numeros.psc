Proceso OperacionesCon2numeros
	Definir n1,n2 Como Entero;
	Definir res Como Real;
	Escribir "dime el numero 1";
	Leer n1;
	Escribir "Dime el numero 2 (evita el 0)";
	Leer n2;
	res<- n1+n2;
	Escribir "la suma de ",n1,"+",n2,"=",res;
	res<- n1-n2;
	Escribir "la resta de ",n1,"-",n2,"=",res;
	res<- n1*n2;
	Escribir "la multiplicacion de ",n1,"*",n2,"=",res;
	res<- n1/n2;
	Escribir "la division de ",n1,"/",n2,"=",res;
FinProceso
